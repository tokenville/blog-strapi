{
  "kind": "collectionType",
  "collectionName": "assistants",
  "info": {
    "singularName": "assistant",
    "pluralName": "assistants",
    "displayName": "Assistant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "welcomemessage": {
      "type": "string",
      "default": "Welcome! You're about to chat with a smart AI assistant skilled in lively, unscripted interviews. But let's pretend you're chatting with a real person. Send your first message now!"
    },
    "interviewer_prompt": {
      "type": "string"
    },
    "clients_brief": {
      "type": "text"
    },
    "publisher_prompt": {
      "type": "string"
    },
    "telegramtoken": {
      "type": "string",
      "unique": true
    },
    "gpt_id": {
      "type": "string",
      "unique": true
    },
    "telegrambot_username": {
      "type": "string",
      "unique": true
    },
    "request": {
      "type": "json"
    },
    "interviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interview.interview",
      "mappedBy": "assistant"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "assistants"
    },
    "bot_name": {
      "type": "string",
      "required": true
    },
    "bot_description": {
      "type": "text"
    },
    "bot_about": {
      "type": "text"
    },
    "botpic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "integration": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::integration.integration",
      "mappedBy": "assistant"
    },
    "reactivation_after": {
      "type": "integer",
      "default": 0,
      "required": false,
      "min": 0,
      "max": 86400
    },
    "base_assistant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::base-assistant.base-assistant",
      "inversedBy": "assistants"
    },
    "tone": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tone.tone",
      "inversedBy": "assistants"
    },
    "slug": {
      "type": "uid",
      "targetField": "bot_name",
      "required": false
    },
    "client_name": {
      "type": "string"
    },
    "client_overview": {
      "type": "text"
    },
    "customised": {
      "type": "boolean",
      "default": false
    },
    "first_message": {
      "type": "json"
    }
  }
}
