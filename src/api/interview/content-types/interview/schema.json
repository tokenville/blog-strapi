{
  "kind": "collectionType",
  "collectionName": "interviews",
  "info": {
    "singularName": "interview",
    "pluralName": "interviews",
    "displayName": "Interview",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "last_review": {
      "type": "datetime"
    },
    "last_length": {
      "type": "integer",
      "default": 0
    },
    "transcript": {
      "type": "json"
    },
    "result": {
      "type": "json"
    },
    "assistant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::assistant.assistant",
      "inversedBy": "interviews"
    },
    "human": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::human.human",
      "inversedBy": "interviews"
    },
    "article": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article.article",
      "inversedBy": "interview"
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "interviews"
    },
    "data_quality": {
      "type": "integer",
      "default": 1,
      "max": 10,
      "min": 1
    },
    "summary": {
      "type": "text",
      "unique": true,
      "default": "Summary is not available yet."
    },
    "title": {
      "type": "string",
      "default": "No Title"
    },
    "authorized": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "chats"
    },
    "thread_id": {
      "type": "uid"
    },
    "attachments": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
